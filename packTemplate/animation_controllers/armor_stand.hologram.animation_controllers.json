{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.armor_stand.hologram.layers": {
			"initial_state": "default",
			"states": {
				"default": {
					"blend_transition": 0.1,
					"blend_via_shortest_path": true,
					"transitions": []
				}
			}
		},
		"controller.animation.armor_stand.hologram.bounding_box": {
			"initial_state": "default",
			"states": {
				"default": {
					"particle_effects": [],
					"transitions": [
						{
							"hidden": "!v.render_hologram"
						}
					]
				},
				"hidden": {
					"transitions": [
						{
							"default": "v.render_hologram"
						}
					]
				}
			}
		},
		"controller.animation.armor_stand.hologram.block_validation": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"validate": "v.validate_hologram"
						}
					]
				},
				"validate": {
					"particle_effects": [],
					"transitions": [
						{
							"default": "!v.validate_hologram"
						}
					]
				}
			}
		}
	}
}