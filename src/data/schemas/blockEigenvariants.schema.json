{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Block Eigenvariants",
	"description": "Eigenvariant definitions for blocks.",
	"type": "object",
	"properties": {
		"$schema": {
			"type": "string"
		}
	},
	"patternProperties": {
		"^\\w+$": {
			"description": "The eigenvariant for this block.",
			"type": "integer",
			"minimum": 0
		}
	},
	"additionalProperties": false
}